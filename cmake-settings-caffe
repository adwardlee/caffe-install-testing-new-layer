#### installing caffe on cluster without sudo
OpenCV_DIR=/home/lijun/dependency/opencv/build/  cmake .. -DUSE_MPI=ON -DBLAS=open -DMPI_CXX_COMPILER="/home/lijun/dependency/openmpi/bin/mpicxx" 
-DBOOST_INCLUDEDIR=/home/lijun/dependency/caffe-deps/include/ -DBOOST_LIBDIR=/home/lijun/dependency/caffe-deps/lib/ 
-DGFLAGS_INCLUDE_DIR=/home/lijun/dependency/caffe-deps/include -DGFLAGS_LIBRARY=/home/lijun/dependency/caffe-deps/lib/libgflags.a 
-DGLOG_INCLUDE_DIR=/home/lijun/dependency/caffe-deps/include/ -DGLOG_LIBRARY=/home/lijun/dependency/caffe-deps/lib/libglog.so 
-DPYTHON_LIBRARY=/home/lijun/dependency/python/lib/libpython2.7.so
-DPYTHON_INCLUDE_DIR=/home/lijun/dependency/python/include/python2.7 
-DCUDA_TOOLKIT_ROOT_DIR=/home/lijun/dependency/cuda-7.5 -DCUDNN_INCLUDE=/home/lijun/dependency/cudnn51/include
-DCUDNN_LIBRARY=/home/lijun/dependency/cudnn51/lib64/libcudnn.so -DPROTOBUF_PROTOC_EXECUTABLE=/usr/bin/protoc -DPROTOBUF_LIBRARY=/usr/lib64/libprotobuf.so
